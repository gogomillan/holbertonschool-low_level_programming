#include "3-calc.h"

/**
 * main - operate two numbers
 *
 * @argc: The qty of parameters
 * @argv: The parameters array
 *
 * Return: The add
 *
 */
int main(int argc, char **argv)
{
	if (argc < 4)
	{
		printf("Error\n");
		return(98);
	}

	printf("[%d]\n", get_op_func(argv[2])(atoi(argv[1]), atoi(argv[3])));
	
	return (0);
}
